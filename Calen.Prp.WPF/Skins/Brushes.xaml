<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    >
    <!--程序框架颜色-->
    <SolidColorBrush x:Key="FullColorKey" Color="DarkGray"/>
    <SolidColorBrush x:Key="PrimaryDarkColorKey" Color="#FFCCCCCC"/>
    <SolidColorBrush x:Key="PrimaryColorKey" Color="#FFF2F2F2"/>
    <SolidColorBrush x:Key="PrimaryLightColorKey" Color="#FFF6F6F6"/>
    <SolidColorBrush x:Key="BlankColorKey" Color="White"/>
    <SolidColorBrush x:Key="TextNormalColorKey" Color="#FF525252"/>
    <SolidColorBrush x:Key="TextHightLightColorKey" Color="Black"/>
    <SolidColorBrush x:Key="DisableColorKey" Color="Gray"/>
</ResourceDictionary>